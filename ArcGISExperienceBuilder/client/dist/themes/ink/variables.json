    }
  },
  "colorationVariants": {
    "default": {
      "header": {
        "bg": "#fff",
        "color": "var(--dark)"
      },
      "body": {
        "bg": "#fff",
        "color": "var(--dark)"
      },
      "footer": {
        "bg": "#fff",
        "color": "var(--dark)"
      }
    }
  },
  "borderRadiuses": {
    "sm": 0,
    "default": 0,
    "lg": 0
  },
  "boxShadows": {
    "sm": "0 0 6px 0 rgba(0,0,0,0.15)",
    "default": "0 0 10px 1px rgba(0,0,0,0.20)",
    "lg": "0 0 20px 2px rgba(0,0,0,0.20)"
  },
  "surfaces": {
    "1": {
      "bg": "#fff",
      "border": {
        "width": "1px",
        "color": "var(--light-800)"
      }
    },
    "2": {
      "border": {
        "width": "1px",
        "color": "var(--light-800)"
      },
      "shadow": "0 0 10px 1px rgba(0,0,0,0.20)"
    },
    "3": {
      "border": {
        "width": "1px",
        "color": "var(--light-800)"
      },
      "shadow": "0 0 6px 0 rgba(0,0,0,0.15)"
    }
  },
  "components": {
    "button": {
      "sizes": {
        "default": {
          "borderRadius": "0"
        },
        "sm": {
          "borderRadius": "0"
        },
        "lg": {
          "borderRadius": "0"
        }
      },
      "variants": {
        "primary": {
          "default": {
            "bg": "var(--primary)",
            "color": "#fff"
          },
          "hover": {
            "bg": "var(--dark-600)"
          }
        },
        "default": {
          "default": {
            "bg": "#fff",
            "color": "var(--dark-800)",
            "border": {
              "color": "var(--light-800)"
            }
          },
          "hover": {
            "bg": "var(--light-100)"
          }
        },
        "secondary": {
          "default": {
            "border": {
              "color": "var(--primary)"
            }
          }
        }
      }
    },
    "nav": {
      "variants": {
        "underline": {
          "size": {
            "paddingY": "0.5rem"
          }
        },
        "tabs": {
          "root": {
            "border": {
              "color": "var(--dark)"
            },
            "borderRadius": 0
          },
          "item": {
            "default": {
              "bg": "var(--light-200)",
              "border": {
                "color": "var(--dark)"
              },
              "borderRadius": 0
            },
            "active": {
              "border": {
                "color": "var(--dark)"
              }
            }
          }
        }
      }
    },
    "input": {
      "border": {
        "color": "var(--light-800)",
        "width": "1px"
      },
      "focus": {
        "borderColor": "var(--primary-700)"
      }
    },
    "modal": {
      "content": {
        "border": {
          "color": "var(--light-300)",
          "width": "1px"
        },
        "borderRadius": 0
      },
      "header": {
        "border": {
          "width": 0
        }
      }
    },
    "pagination": {
      "variants": {
        "default": {
          "button": {
            "default": {
              "border": {
                "color": "var(--light-800)"
              }
            },
            "hover": {
              "bg": "var(--light-100)",
              "border": {
                "color": "var(--light-800)"
              }
            }
          }
        }
      }
    }
  },
  "arcgis": {
    "boxShadow": "0 0 6px 0 rgba(0,0,0,0.15)",
    "components": {
      "button": {
        "variants": {
          "default": {
            "default": {
              "bg": "#fff",
              "border": {
                "color": "var(--light-800)",
                "width": "1px"
              }
            },
            "disabled": {
              "bg": "#fff"
            }
          }
        }
      }
    }
  }
}